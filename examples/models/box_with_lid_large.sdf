          <sphere>
            <radius>0.17</radius>
          </sphere>  
          <!--
          <box>
            <size>0.41 0.315 0.2604</size>
          </box>
          -->
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.5</mu>
              <mu2>0.5</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <collision name="minus_x_lower_left_corner">
